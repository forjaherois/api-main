generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id           String   @id
  nickname     String
  email        String   @unique
  password     String
  is_confirmed Boolean
  created_at   DateTime @default(now())

  @@map("accounts")
}
